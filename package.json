{
  "name": "ng-pipes",
  "version": "0.1.1",
  "description": "a8m/angular-filter for Angular 2",
  "files": [
    "src",
    "dist/*.js",
    "index.d.ts",
    "tsconfig.json"
  ],
  "main": "dist/ng-pipes.umd.js",
  "scripts": {
    "prepublish": "node_modules/.bin/gulp clean && npm run ngc && npm run build",
    "tsc": "node_modules/.bin/tsc -p .",
    "karma": "node_modules/.bin/karma start karma.conf.js",
    "test": "npm run lint && npm run ngc && npm run karma",
    "ngc": "node_modules/.bin/ngc -p tsconfig.json",
    "build": "npm run format && npm run tsc && npm run rollup",
    "rollup": "./node_modules/.bin/rollup -c -o dist/ng-pipes.umd.js",
    "postrollup": "./node_modules/.bin/uglifyjs ./dist/ng-pipes.umd.js > ./dist/ng-pipes.umd.min.js",
    "format": "./node_modules/.bin/clang-format --glob=\"{@(src|test)/**/*.ts,*.js}\" -i",
    "lint": "tslint '{src,test}/**/*.ts' -e '**/*.d.ts'"
  },
  "author": "Ariel Mashraki",
  "repository": {
    "type": "git",
    "url": "https://github.com/a8m/ng-pipes.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "angularjs",
    "pipes",
    "filters",
    "angular-filter",
    "angular-filters",
    "ng-pipes",
    "ng",
    "a8m",
    "lodash",
    "underscore"
  ],
  "license": "MIT",
  "devDependencies": {
    "@angular/compiler": "^2.3.0",
    "@angular/compiler-cli": "^2.3.0",
    "@angular/core": "^2.3.0",
    "@angular/platform-server": "^2.3.0",
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "^2.5.37",
    "clang-format": "^1.0.46",
    "codelyzer": "^2.0.0",
    "core-js": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "reflect-metadata": "^0.1.3",
    "rollup": "^0.41.4",
    "rollup-plugin-typescript": "^0.8.1",
    "rxjs": "5.0.0-rc.4",
    "symbol-observable": "^0.2.4",
    "tslint": "^4.4.2",
    "typescript": "^2.1.4",
    "uglify-js": "^2.7.5",
    "webpack": "^1.13.3",
    "zone.js": "^0.7.2"
  },
  "dependencies": {}
}